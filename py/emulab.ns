#
# Anon protocol NS file
#

set ns [new Simulator]                  
source tb_compat.tcl

set maxnodes 3
set lanstr ""

for {set i 1} {$i <= $maxnodes} {incr i} {
    set node($i) [$ns node]
    append lanstr "$node($i) "
    tb-set-node-os $node($i) UBUNTU70-STD
}

set big-lan [$ns make-lan "$lanstr" 100Mb 0ms]

$ns rtproto Static
$ns run           

